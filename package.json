{
    "name": "next-ai-optimizer",
    "version": "0.1.0",
    "description": "Optimization layer for Next.js applications to enhance AI agent interaction",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./react": {
            "import": "./dist/react/index.js",
            "require": "./dist/react/index.js",
            "types": "./dist/react/index.d.ts"
        },
        "./babel": "./dist/babel/plugin.js",
        "./next": "./dist/next/config.js"
    },
    "scripts": {
        "build": "tsc && babel src --out-dir dist --copy-files --no-copy-ignored",
        "test": "jest",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "lint": "eslint src/**/*.js src/**/*.jsx"
    },
    "keywords": [
        "nextjs",
        "react",
        "ai",
        "agent",
        "automation",
        "accessibility"
    ],
    "author": "whyismynamerudy",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/whyismynamerudy/next-ai-optimizer.git"
    },
    "peerDependencies": {
        "next": ">=12.0.0",
        "react": ">=17.0.0",
        "react-dom": ">=17.0.0"
    },
    "dependencies": {
        "@babel/core": "^7.21.0",
        "@babel/helper-plugin-utils": "^7.20.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/plugin-transform-runtime": "^7.26.9",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@types/react": "^19.0.10",
        "babel-jest": "^29.5.0",
        "eslint": "^8.36.0",
        "eslint-plugin-react": "^7.32.2",
        "jest": "^29.5.0",
        "next": "^13.2.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.7.3"
    },
    "optionalDependencies": {
        "openai": "^4.0.0"
    }
}
